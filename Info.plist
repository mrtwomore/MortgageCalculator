			<string>com.mortgagecalculator.scenario</string>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>mortgagescenario</string>
				</array>
				<key>public.mime-type</key>
				<array>
					<string>application/x-mortgagescenario</string>
				</array>
			</dict>
		</dict>
	</array>
</dict>
</plist> 